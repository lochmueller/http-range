{
  "name": "lochmueller/http-range-partial-download",
  "autoload": {
    "psr-4": {
      "Lochmueller\\HttpRange\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lochmueller\\HttpRange\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Tim Lochm√ºller",
      "email": "tim.lochmueller@hdnet.de"
    }
  ],
  "require": {
    "php": "^8.1",
    "psr/http-server-handler": "^1.0",
    "ramsey/http-range": "dev-main",
    "php-http/discovery": "^1.18",
    "psr/http-factory-implementation": "*",
    "php-http/multipart-stream-builder": "^1.3",
    "php-http/message": "^1.16",
    "php-http/curl-client": "^2.3",
    "guzzlehttp/psr7": "^2.5"
  },
  "require-dev": {
    "nyholm/psr7": "^1.8",
    "phpunit/phpunit": "^10.2",
    "friendsofphp/php-cs-fixer": "^3.17"
  },
  "scripts": {
    "test": "phpunit tests",
    "php-cs-fixer-config": "php-cs-fixer fix --rules=@PER,no_unused_imports,ordered_imports,dir_constant --allow-risky=yes",
    "code-fix": [
      "@php-cs-fixer-config src/",
      "@php-cs-fixer-config tests/"
    ]
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": false
    }
  }
}
