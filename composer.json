{
  "name": "lochmueller/http-range",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Lochmueller\\HttpRange\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lochmueller\\HttpRange\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Tim Lochm√ºller",
      "email": "tim.lochmueller@hdnet.de"
    }
  ],
  "require": {
    "php": "^8.1",
    "psr/http-server-handler": "^1.0",
    "psr/http-factory-implementation": "*",
    "psr/http-server-middleware": "^1.0",
    "php-http/curl-client": "^2.3",
    "ramsey/http-range": "^1.1"
  },
  "require-dev": {
    "nyholm/psr7": "^1.8",
    "phpunit/phpunit": "^10.2",
    "friendsofphp/php-cs-fixer": "^3.17",
    "phpstan/phpstan": "^1.10"
  },
  "scripts": {
    "test": "XDEBUG_MODE=coverage phpunit --coverage-text --coverage-html=var/phpunit/ --configuration=config/phpunit.xml.dist",
    "test-large": "@test --group=large",
    "code-fix": "php-cs-fixer fix --config=config/.php-cs-fixer.dist.php",
    "code-check": "phpstan analyse src tests --level=7"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
